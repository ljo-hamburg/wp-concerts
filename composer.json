{
  "name": "ljo-hamburg/wp-concerts",
  "description": "Concerts for WordPress.",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": "^7.4|^8.0",
    "yahnis-elsts/plugin-update-checker": "^4.11",
    "ext-intl": "*",
    "ext-json": "*",
    "jsvrcek/ics": "^0.8"
  },
  "require-dev": {
    "johnpbloch/wordpress": "^5.8.3",
    "squizlabs/php_codesniffer": "^3.6.2",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "wp-coding-standards/wpcs": "^2.3.0",
    "wp-cli/wp-cli": "2.5.0",
    "wp-cli/i18n-command": "^2.2.11"
  },
  "authors": [
    {
      "name": "Kim Wittenburg",
      "email": "codello@wittenburg.kim",
      "homepage": "https://github.com/codello"
    }
  ],
  "autoload": {
    "psr-4": {
      "LJO\\WPConcerts\\": "includes/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
